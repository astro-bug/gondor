<configuration>

  <id>dev-gondor</id>
  <name># Gondor Web Server</name>
  <description>Gondor WEB服务</description>
  
  <workingdirectory>D:\gondor</workingdirectory>
  <executable>D:\gondor\gondor.exe</executable>
  <arguments>run</arguments>
  <logmode>reset</logmode>
  
  <onfailure action="restart" delay="10 sec" />
  <onfailure action="restart" delay="20 sec" />
  <onfailure action="none" />

</configuration>
